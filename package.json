{
  "name": "shassi-frontend",
  "version": "1.6.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run ng -- serve --proxy-config proxy.conf.json --aot",
    "start:de": "npm run start -- --i18n-file=src/i18n/messages.de.xlf --locale=de --i18n-format=xlf",
    "start:sw": "npm run build && npm run start:liteserver",
    "start:liteserver": "lite-server -c liteserver_config.js",
    "dev-server": "json-server -w mock-backend.json --middlewares mock-backend.middleware.js",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e -- --protractor-config=e2e/protractor-ci.conf.js",
    "e2e-dev": "ng e2e --dev-server-target="
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/cdk": "7.3.7",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/pwa": "^0.13.9",
    "@angular/router": "8.0.0",
    "@angular/service-worker": "8.0.0",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/entity": "^7.4.0",
    "@ngrx/schematics": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "core-js": "^2.6.8",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "8.0.1",
    "@angular/compiler-cli": "8.0.0",
    "@angular/language-service": "8.0.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.0.2",
    "codelyzer": "^5.0.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-server": "^0.15.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "ts-node": "^8.1.1",
    "tslint": "^5.16.0",
    "typescript": "3.4.5"
  }
}
